  "assembly": {
    "mnemonics": {
      "push": ["push <number>", "push <char>"],
      "dup": ["dup"],
      "copy": ["copy"],
      "swap": ["swap"],
      "drop": ["pop"],
      "slide": ["slide"],
      "add": ["add"],
      "sub": ["sub"],
      "mul": ["mul"],
      "div": ["div"],
      "mod": ["mod"],
      "store": ["store"],
      "retrieve": ["load"],
      "label": ["<number>:", "label"],
      "call": ["call"],
      "jmp": ["jump"],
      "jz": ["jz"],
      "jn": ["jn"],
      "ret": ["ret"],
      "end": ["exit"],
      "printc": ["ochr"],
      "printi": ["onum"],
      "readc": ["ichr"],
      "readi": ["inum"]
    },
    "patterns": { "char": "." },
    "line_comments": [";"],
    "indentation": "    ",
    "usage": ["assembler", "programs"],
    "extension": "asm"
  },
