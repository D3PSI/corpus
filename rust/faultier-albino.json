{
  "id": "rust/faultier-albino",
  "name": "Albino",
  "authors": ["Taro Sako"],
  "license": "MIT",
  "languages": ["Rust"],
  "tags": ["interpreter", "assembler", "disassembler", "bytecode", "Brainfuck to Whitespace", "programs"],
  "date": "2014-07-18 09:13:55 +0900",
  "spec_version": "0.3",
  "source": ["https://github.com/faultier/albino"],
  "relations": [{ "id": "rust/faultier-whitebase", "type": "library" }],
  "whitespace": { "extension": "ws" },
  "assembly": {
    "mnemonics": {
      "push": ["PUSH"],
      "dup": ["DUP"],
      "copy": ["COPY"],
      "swap": ["SWAP"],
      "drop": ["DISCARD"],
      "slide": ["SLIDE"],
      "add": ["ADD"],
      "sub": ["SUB"],
      "mul": ["MUL"],
      "div": ["DIV"],
      "mod": ["MOD"],
      "store": ["STORE"],
      "retrieve": ["RETRIEVE"],
      "label": ["MARK"],
      "call": ["CALL"],
      "jmp": ["JUMP"],
      "jz": ["JUMPZ"],
      "jn": ["JUMPN"],
      "ret": ["RETURN"],
      "end": ["EXIT"],
      "printc": ["PUTC"],
      "printi": ["PUTN"],
      "readc": ["GETC"],
      "readi": ["GETN"]
    },
    "usage": ["assembler", "disassembler"]
  },
  "programs": [
    { "path": "examples/fib.ws", "spec_version": "0.2" },
    { "path": "examples/hello.ws", "equivalent": "hworld.ws", "spec_version": "0.2" }
  ],
  "build": {
    "base_image": "rust:1.61",
    "targets": [
      {
        "binaries": [
          "target/release/albino",
          "target/release/albino-run",
          "target/release/albino-build",
          "target/release/albino-exec",
          "target/release/albino-gen"
        ],
        "build": "cargo build --release"
      },
      {
        "binaries": [
          "target/debug/albino",
          "target/debug/albino-run",
          "target/debug/albino-build",
          "target/debug/albino-exec",
          "target/debug/albino-gen"
        ],
        "build": "cargo build"
      }
    ]
  },
  "commands": [
    {
      "bin": "target/release/albino",
      "build_errors": "Needs many changes for latest Rust",
      "option_parse": "manual"
    },
    {
      "bin": "target/release/albino-run",
      "build_errors": "Needs many changes for latest Rust",
      "option_parse": "manual"
    },
    {
      "bin": "target/release/albino-build",
      "build_errors": "Needs many changes for latest Rust",
      "option_parse": "manual"
    },
    {
      "bin": "target/release/albino-exec",
      "build_errors": "Needs many changes for latest Rust",
      "option_parse": "manual"
    },
    {
      "bin": "target/release/albino-gen",
      "build_errors": "Needs many changes for latest Rust",
      "option_parse": "manual"
    }
  ],
  "todo": "Document options"
}
